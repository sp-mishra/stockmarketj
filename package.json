{
    "name": "stockmarket4j-cds",
    "version": "1.0.0",
    "description": "Generated by cds-services-archetype",
    "license": "ISC",
    "repository": "<Add your repository here>",
    "scripts": {
        "deploy": "cds-deploy"
    },
    "cds": {
        "build": {
            "target": ".",
            "tasks": [
                {
                    "for": "nodejs",
                    "src": "db",
                    "options": {
                        "model": [
                            "db",
                            "srv"
                        ]
                    }
                },
                {
                    "for": "java",
                    "src": "srv",
                    "options": {
                        "model": [
                            "db",
                            "srv"
                        ]
                    }
                }
            ]
        },
        "requires": {
            "db": {
                "kind": "postgres",
                "impl": "@cap-js/postgres",
                "pool": {
                    "acquireTimeoutMillis": 3000
                },
                "vcap": {
                    "label": "postgresql-db"
                }
            }
        }
    },
    "dependencies": {
        "@cap-js/postgres": "^1.8.0",
        "@sap/cds": "^7",
        "@sap/cds-dk": "^8.3.0"
    },
    "devDependencies": {
        "cds-plugin-ui5": "^0.9.3"
    }
}
